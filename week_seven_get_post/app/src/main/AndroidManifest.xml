<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mudiataog.getpost">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GETPOST">
        <activity android:name="com.mudiataog.getpost.ui.SplashActivity"
            android:theme="@style/Theme.GETPOST.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mudiataog.getpost.activity.AddStudentActivity" />
        <activity android:name="com.mudiataog.getpost.MainActivity">

        </activity>
    </application>

</manifest>